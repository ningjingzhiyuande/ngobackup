  <div class="two_third lastcolumn contact1">
    <div id="contactForm">
      <h2>留言</h2>
      <div class="sepContainer"></div>
      <div class="msg"></div>
      <form action="/contacts" method="post" id="contact_form">

          <input type="hidden" name="contact[item_type]" value="Message"/>
        <div class="name">
          <label for="name">Your Name:</label>
         <!-- <p> Please enter your full name</p>-->
          <input id="name" name="contact[name]" type="text" placeholder="e.g. Mr. John Smith" required />
        </div>
        <div class="email">
          <label for="email">Your Email:</label>
         <!-- <p> Please enter your email address</p>-->
          <input id=email name="contact[email]" type=email placeholder="example@domain.com" required />
        </div>
        <div class="message">
          <label for="message">Your Message:</label>
        <!--  <p> Please enter your question</p>-->
          <textarea id=message name="contact[content]" rows=6 cols=10 required></textarea>
        </div>
        <div id="loader">
          <input type="submit" value="提交留言" class="submit_contact_form" />
        </div>
      </form>
    </div>
