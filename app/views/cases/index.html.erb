<div class="blankSeparator"></div>
<div class="container portfolio">
  <%@cases.each do |c|%>
  <div class="two_third">
    <div class="flexslider">
      <ul class="slides">
        <%c.assets.each do |asset|%>
          <li><%=image_tag asset.avatar.url%></li>
        <%end%>
      </ul>
    </div>
  </div>
  <div class="one_third lastcolumn">
    <h2><%=c.title%></h2>
    <div class="case_content"><%=c.content%></div>
    <p class="portfolio"><a href="<%=c.url%>">Visit Website</a></p>
  </div>
  <%end%>
</div>
